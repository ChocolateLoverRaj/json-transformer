{
  "name": "@programmerraj/json-transformer",
  "version": "1.0.0",
  "description": "Modify json values with plugins.",
  "main": "dist/umd/index.js",
  "exports": {
    ".": {
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.js"
    },
    "./": "./"
  },
  "scripts": {
    "lint": "eslint .",
    "build:umd": "babel lib --extensions .ts --out-dir dist/umd --presets @babel/preset-typescript --plugins @babel/plugin-transform-modules-umd",
    "build:es": "babel lib --extensions .ts --out-dir dist/es --presets @babel/preset-typescript",
    "test": "tsc && mocha dist/test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chocolateloverraj/json-transformer.git"
  },
  "keywords": [
    "json",
    "transformer"
  ],
  "author": "Rajas Paranjpe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chocolateloverraj/json-transformer/issues"
  },
  "homepage": "https://github.com/chocolateloverraj/json-transformer#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-transform-modules-umd": "^7.13.0",
    "@babel/preset-typescript": "^7.13.0",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.32",
    "@types/sinon": "^9.0.11",
    "eslint": "^7.21.0",
    "mocha": "^8.3.1",
    "sinon": "^9.2.4",
    "ts-standard": "^10.0.0",
    "typescript": "^4.2.3"
  }
}